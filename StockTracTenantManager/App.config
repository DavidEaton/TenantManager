<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<appSettings>
		<!-- 
      The server name of the database to connect to. Do not include the https:// prefix of the Azure SQL DB server URL.
      Typically, the server name looks like this: abcdefghij.database.windows.net
      
      Edit this field before running the application.
    -->

		<add key="ServerName" value="localhost" />
		<!--<add key="ServerName" value="janco-tenants.database.windows.net" />-->
		<add key="ShardMapName" value="StockTracShardMap" />
		<add key="ShardMapManagerDatabaseName" value="StockTracTenantManager" />

		<add key="IdentityTenantsServerName" value="localhost" />
		<!--<add key="IdentityTenantsServerName" value="janco-idp.database.windows.net" />-->
		<add key="IdentityTenantsDatabaseName" value="JancoIdentity" />

		<!--
      Set to true to enable Windows Authentication instead of SQL Authentication.
      This is useful only if connecting to a local SQL Server database instead of Azure SQL DB.
    -->
		<add key="IntegratedSecurity" value="true" />

		<!-- 
      Credentials for connecting to your Azure SQL DB server.
      Edit these fields before running the application.
    -->
		<add key="UserName" value="jancoAdmin@janco-tenants" />
		<add key="Password" value="adminPassword" />
		<add key="UserNameIdentityUsers" value="jancoAdmin@janco-idp" />
		<add key="PasswordIdentityUsers" value="adminPassword" />

		<!--
      The database edition to use when creating databases for this sample in Azure SQL DB.
    -->
		<add key="DatabaseEdition" value="standard" />
		<!--
      The pricing tier, aka performance level, aka SERVICE_OBJECTIVE to use when creating databases in Azure SQL DB,
      or the Elastic Pool Name. Databases created in pool takes the pricing tier of its parent pool
    -->
		<add key="ServiceObjective" value="ELASTIC_POOL ( name = StockTrac )" />

		<!--
      The Connection String to use when working with Azure Storage.
    -->
		<add key="AzureStorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=stocktracstorage;AccountKey=XuTHPlqcGYxE5csmYpYlpqbHIf9fJs8U9298DEkXrkWr1CA+RLyr9qIVIkCKHEm+OD+tRhWyJVdKvq76bADJcw==;EndpointSuffix=core.windows.net" />
	</appSettings>	
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
